{% set workshop_image_width  = page.header.header_image_width|defined(400) %}{% set workshop_image_height = page.header.header_image_height|defined(400) %}{% set tag_classes = '' %}{% if page.taxonomy.tag %}  {% set tag_classes = [] %}  {% for tag in page.taxonomy.tag %}    {% set tag_classes = tag_classes|merge(["tag-%s"|format(tag|replace(' ', '-'))]) %}  {% endfor %}  {% set tag_classes = tag_classes|join(',') %}{% endif %}<!-- Workshop Card -->{% if page.header.link %}  {% if page.header.continue_link is not sameas(false) %}    {% set page_link = '#' %}  {% endif %}{% else %}  {% set page_link = page.url %}{% endif %}<div data-uk-filter="{{tag_classes}}">  <div class="uk-panel-box workshop-item" data-uk-scrollspy="{cls:'uk-animation-fade'}">    <!-- Workshop title -->    <div class="uk-panel-title"><h2>{{ page.title }}</h2></div>    <!-- Workshop images-->    {% if page.header.image|defined(true) %}      {{ page.media.images|first.cropZoom(workshop_image_width, workshop_image_height).html( page.title, page.title) }}    {% endif %}    <div class="uk-text-left">{{ page.content }}</div>    {# <div class=""> <!-- meta style? --> #}    <!--Workshop tags-->    {% if page.taxonomy.tag %}      <ul class="uk-subnav uk-subnav-line">        <i class="uk-icon-tag uk-hidden-small"></i>      {% for tag in page.taxonomy.tag %}        <li>{{tag}}</li>      {% endfor %}      </ul>    {% endif %}    <!-- Call to action button -->      <button class="uk-button uk-button-primary uk-width-1-1">        <i class='uk-icon-user-plus'></i> Sign up for this workshop!      </button>    {# </div> #}  </div></div>